CREATE TABLE 회원(

ID VARCHAR(20) NOT NULL,

PW VARCHAR(20) NOT NULL,

PHONE VARCHAR(20) NOT NULL,

PRIMARY KEY(ID)

)

 

COMMIT;

CREATE TABLE 피시방(

PC NUMBER NOT NULL,

TOTAL NUMBER NOT NULL,

날짜 VARCHAR(20),

PRIMARY KEY(PC)

)

COMMIT;

 

CREATE TABLE 관리자(

ID VARCHAR(20) NOT NULL,

PW VARCHAR(20) NOT NULL,

PHONE VARCHAR(20) NOT NULL,

PRIMARY KEY(ID)

)

INSERT INTO 관리자 VALUES ('system','123456','01099501471');

 

CREATE TABLE 피시방좌석(

PC NUMBER NOT NULL,

SEAT NUMBER NOT NULL,

예약유무 NUMBER,

PRIMARY KEY(PC,SEAT),

FOREIGN KEY(PC) REFERENCES 피시방(PC)

)

 

CREATE TABLE 예약좌석(

ID VARCHAR(20) NOT NULL,

PC NUMBER NOT NULL,

SEAT NUMBER,

PRIMARY KEY(ID,PC)

)

INSERT INTO 피시방 VALUES (1,100,20210615);

COMMIT;
